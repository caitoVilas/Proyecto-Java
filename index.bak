  <!-- =============   MAIN   ========================= -->
  <main>
    <h1 class="titulo-principal">Curso Java Comision 2011</h1>
    <h3 class="text-center subtitulo mb-5">codo <i class="fas fa-at"></i> codo</h3>
    <!-- Contenido sin loggin     -->
    <div class="container sin_loggin">
      <div class="jumbotron jumbotron-fluid">
        <div class="container">
          <h1 class="display-4 text-center text-warning">Java Full-Stack Comision 2011</h1>
          <p class="lead text-center text-dark">Profesor : Mauricio Sanchez (Java Guru)</p>
        </div>
      </div>
    </div>
    <aside>
      <div class="panel">
        <div class="container mt-5">
          <h3 class="text-center">Usuarios</h3>
          <ul class="nav nav-tabs" id="myTab" role="tablist">
            <li class="nav-item" role="presentation">
              <a class="nav-link active" id="home-tab" data-toggle="tab" href="#home" role="tab" aria-controls="home"
                aria-selected="true">Registro</a>
            </li>
            <li class="nav-item" role="presentation">
              <a class="nav-link" id="profile-tab" data-toggle="tab" href="#profile" role="tab" aria-controls="profile"
                aria-selected="false">Loggin</a>
            </li>
          </ul>
          <div class="tab-content" id="myTabContent">
            <div class="tab-pane fade show active" id="home" role="tabpanel" aria-labelledby="home-tab">
              ...Registro
              <!-- panel registro -->
              <form id="frm_registro" class="frm_registro">
                <div class="container">
                  <div class="row text-center">
                    <div class="col-md-12">
                      <div class="form-group fgr">
                        <input type="text" id="r_name" name="name" placeholder="Ingrese su nombre"
                          title="Debes ingresar tu nombre para el registro">
                        <br><span id="s_name" class="none"></span>
                      </div>
                    </div>
                  </div>
                  <div class="row text-center">
                    <div class="col-md-12">
                      <div class="form-group fgr">
                        <input type="email" id="r_mail" name="email" placeholder="Ingrese su email"
                          pattern="^[a-z0-9]+(\.[_a-z0-9]+)*@[a-z0-9-]+(\.[a-z0-9-]+)*(\.[a-z]{2,15})$"
                          title="La direccion de email no tiene formato correcto">
                        <br><span id="s_mail" class="none"></span>
                      </div>
                    </div>
                  </div>
                  <div class="row text-center">
                    <div class="col-md-12">
                      <div class="form-group fgr">
                        <input type="text" id="r_uname" name="username" placeholder="nombre usuario"
                          title="Necesitamos tu nombre de usuario para registrarte">
                        <br><span id="s_unam" class="none"></span>
                      </div>
                    </div>
                  </div>
                  <div class="row text-center">
                    <div class="col-md-12">
                      <div class="form-group fgr">
                        <input type="password" id="r_pass" name="password" placeholder="Ingrese su contraseña"
                          title="Necesitamos tu contraseña para registrarte">
                        <br><span id="s_pass" class="none"></span>
                      </div>
                    </div>
                  </div>
                  <div class="container text-center none" id="loader">
                    <div class="spinner-border text-primary" role="status">
                      <span class="sr-only">Loading...</span>
                    </div>
                  </div>
                  <div class="row text-center">
                    <div class="col-md-12">
                      <div class="form-group">
                        <button class="btn btn-outline-success btn-block" type="submit" id="ok_nuevo">
                          <i class="far fa-clipboard"></i>
                          Registrar</button>
                      </div>
                      <div class="row text-center">
                        <div class="col-md-12">
                          <div class="form-group">
                            <button class="btn btn-outline-danger btn-block btn_cancelar" type="button"
                              id="cancel_nuevo">
                              <i class="fas fa-ban"></i>
                              Cancelar</button>
                          </div>
                        </div>

              </form>

            </div>
            <!--cierra registro-->
            <div class="tab-pane fade" id="profile" role="tabpanel" aria-labelledby="profile-tab">
              ...Loggin

              <!-- Panel Loggin -->
              <form id="frm_loggin" class="frm_loggin">
                <div class="container">
                  <div class="row text-center">
                    <div class="col-md-12">
                      <div class="form-group">
                        <input type="text" name="nombreUsuario" placeholder="nombre usuario"
                          title="Debes ingresar nombre de usuario" id="user" required>
                      </div>
                    </div>
                  </div>
                  <div class="row text-center">
                    <div class="col-md-12">
                      <div class="form-group">
                        <input type="password" name="password" placeholder="Ingrese su contraseña"
                          title="Debes ingresar tu contraseña" id="pass" required>
                      </div>
                    </div>
                  </div>
                  <div class="row text-center">
                    <div class="col-md-12">
                      <div class="form-group">
                        <button class="btn btn-outline-success btn-block b_ing" type="submit" id="btn_ingresar">
                          <i class="fas fa-sign-in-alt"></i>
                          Ingresar</button>
                      </div>
                    </div>
                  </div>
                  <div class="row text-center">
                    <div class="col-md-12">
                      <div class="form-group">
                        <button class="btn btn-outline-danger btn-block btn_cancelar">
                          <i class="fas fa-ban"></i>
                          Cancelar</button>
                      </div>
                    </div>
                  </div>
                </div>
              </form>
            </div>
            <!--cierra registro-->

          </div>
          <!--cierra loggin-->
        </div>
        <!--cierra tab content-->
      </div>
      <!--cierra conteiner-->
      </div>
